<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="app.wiper.mapper.interfaces.VehicleAddressMapper">
    <resultMap id="vehicleAddress" type="app.wiper.domain.core.VehicleAddress">
        <result property="parkingLotNo" column="entity_type_id" jdbcType="VARCHAR" />
        <result property="floor" column="name" jdbcType="VARCHAR" />
        <result property="building" column="description" jdbcType="VARCHAR" />
        <result property="city" column="order_id" jdbcType="VARCHAR" />
        <result property="state" column="is_active" jdbcType="VARCHAR" />
        <result property="pin" column="pin" jdbcType="INTEGER" />
    </resultMap>
    <select id="getAddressForVehicle" resultMap="vehicleAddress">
        select * from vehicle_address where vehicle_id = #{vehicleId}
    </select>
</mapper>